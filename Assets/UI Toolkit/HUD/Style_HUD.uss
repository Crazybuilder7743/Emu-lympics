    height: 229px;
    min-width: 530px;
    max-width: 530px;
    min-height: 1px;
    max-height: 229px;
    scale: 0.35 0.35;
    transform-origin: left top;
    -unity-background-scale-mode: scale-and-crop;
    background-position-y: top 0;
    background-position-x: left 0;
    left: 192px;
    top: 91px;
    -unity-background-image-tint-color: rgb(219, 53, 204);
    rotate: 180deg;
}

