}

.speedometer {
    width: 200px;
    min-width: 200px;
    max-width: 200px;
    height: 10px;
    min-height: 10px;
    max-height: 10px;
    transform-origin: center;
    rotate: 0;
}

.speedometerDing {
    flex-grow: 1;
    width: 50px;
    min-width: 50px;
    max-width: 3050px;
    height: 10px;
    min-height: 10px;
    max-height: 10px;
