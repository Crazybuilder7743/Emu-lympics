    margin-bottom: 0;
    margin-left: 0;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.healthBarContainer {
    width: 600px;
    height: 35px;
    min-width: 600px;
    max-width: 600px;
    min-height: 35px;
    max-height: 35px;
    top: 25px;
}

.speedometerContainer {
    position: absolute;
    width: 200px;
