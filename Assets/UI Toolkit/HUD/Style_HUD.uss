}

.eyes {
    width: 530px;
    height: 229px;
    min-width: 530px;
    max-width: 530px;
    min-height: 229px;
    max-height: 229px;
    scale: 0.35 0.35;
    transform-origin: left top;
    background-position-y: top 0;
    background-position-x: left 0;
    left: 192px;
    top: 91px;
    position: absolute;
    rotate: 180deg;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-image: url("project://database/Assets/03_Art(HabenwirJaNetVieles)/Eyes.png?fileID=2800000&guid=838f435bab8176f44959d459f01217ed&type=3#Eyes");
